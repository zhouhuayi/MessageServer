<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
       "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.choose.Message.mapper.CommonMapper">
	<select id="getMapListClass1" resultType="java.util.Map"
		parameterType="com.choose.Message.util.Params">
		select
		<foreach collection="columns" item="column" separator=",">
			${column}
		</foreach>
		from ${tables} where
		<if test="null ==whereSql">
			1=1
		</if>
		<if test="null != whereSql">
			${whereSql}
		</if>
		<if test="null != paramList and paramList.size!=0">
			<foreach collection="paramList" item="item" index="index">
				<if test="null!=item.value">
					${item.connSymbol} ${item.column} ${item.judgeSymbol}
					<if test="item.judgeSymbol.equals('in')">
						<foreach collection="item.value" item="columnValue" index="index"
							open="(" separator="," close=")">
							#{columnValue}
						</foreach>
					</if>

					<if test="!item.judgeSymbol.equals('in')">
						#{item.value}
					</if>

					<if test="null!=item.symbolEnd">
						${item.symbolEnd}
					</if>
				</if>
			</foreach>
		</if>
		<if test="sortColumn != null">
			order by ${sortColumn} ${sort}
		</if>
		<if test="end">
			limit ${start},${end}
		</if>
	</select>

