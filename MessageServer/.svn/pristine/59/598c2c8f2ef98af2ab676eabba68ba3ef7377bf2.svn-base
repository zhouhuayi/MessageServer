	<insert id="add1" parameterType="com.choose.Message.util.Params"
		useGeneratedKeys="true" keyProperty="id">
		insert into ${tables}
		<foreach collection="insertMap.keys" item="k" index="index"
			open="(" separator="," close=")">
			<if test="null != insertMap[k]">${k}</if>
		</foreach>
		values
